# Serveur
PORT=4000
NODE_ENV=development

# Sécurité
JWT_SECRET=change-me-to-a-secure-random-string-min-32-chars
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-me-to-secure-password

# API IA (choisir un des deux)
OPENAI_API_KEY=sk-...
DEEPSEEK_API_KEY=sk-...
AI_PROVIDER=openai

# Configuration Agent
AGENT_WORKSPACE=/opt/agent-projects
ALLOWED_DOCKER_NETWORKS=agent-network
MAX_CONCURRENT_TASKS=3

# Sécurité avancée
ENABLE_DRY_RUN=true
REQUIRE_APPROVAL=true
ALLOWED_COMMANDS=docker,npm,node,git,mkdir,chmod

# Paiements (Stripe)
STRIPE_PUBLIC_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
